{
    "extends": "airbnb",
    "rules": {
        "jsx-quotes": [2, "prefer-single"],
        "react/jsx-curly-spacing": [2, "always"],
        "react/prefer-stateless-function": [0],
        "comma-dangle": "off",
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4]
    },
    "env": {
        "jasmine": true,
        "browser": true
    }
}